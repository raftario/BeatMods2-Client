{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended",
    "tslint-config-standard"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "rules": {
    "ordered-imports": true,
    "object-literal-sort-keys": false,
    "max-line-length": [
      true,
      160
    ],
    "arrow-parens": false
  }
}
